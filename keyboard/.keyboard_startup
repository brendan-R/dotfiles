#!/bin/sh

# . /etc/X11/Xsession

xmodmap ~/.Xmodmap

# The below allows the keys to repeat...

xset r 111 # Up
xset r 113 # Left
xset r 116 # Down
xset r 114 # Right
xset r 115 # End
xset r 110 # Home

xset r 91 # Delete
xset r 81 # Page Up
xset r 89 # Page Down
xset r 89 # Page Down
xset r 22 # Space
xset r 65 # Backspace

# xset r 37 # Left Control

xset r 31 # i
xset r 43 # h
xset r 44 # j
xset r 45 # k
xset r 46 # l
# xset r 39 # s
xset r 47 # ;
xset r 33 # p
xset r 32 # o
xset r 30 # u
xset r 41 # f
xset r 58 # m
